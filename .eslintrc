{
    "extends": "prettier",
    "parserOptions": {
        "ecmaVersion": 2019,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": ["prettier"],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "max-len":["error", 140, 4],
        "prettier/prettier":["error",{"trailingComma":"es5", "singleQuote":true}]
    }
}